    "def distance(dipole, point):\n",
    "    xd = dipole[0]-point[0]\n",
    "    yd = dipole[1] - point[1]\n",
    "    zd = dipole[2] - point[2]\n",
    "    return math.sqrt(xd**2 + yd**2 + zd**2)"
